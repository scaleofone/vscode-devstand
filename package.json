{
  "private": true,
  "name": "kitchen-sink",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/extension/extension.cjs",
  "browser": "./dist/extension/extension.js",
  "engines": {
    "vscode": "^1.60.0"
  },
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "KitchenSink.openWithBreadboardEditor",
          "group": "navigation",
          "when": "resourceFilename =~ /breadboard\\.jsonnet$/"
        },
        {
          "command": "KitchenSink.openWithEditor",
          "group": "navigation",
          "when": "resourceFilename =~ /\\.point\\.json$/"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "KitchenSink.BreadboardEditorProvider",
        "displayName": "Breadboard editor",
        "selector": [
          {
            "filenamePattern": "*breadboard.jsonnet"
          }
        ],
        "priority": "option"
      },
      {
        "viewType": "KitchenSink.EditorProvider",
        "displayName": "KitchenSink editor",
        "selector": [
          {
            "filenamePattern": "*.point.json"
          }
        ],
        "priority": "option"
      }
    ],
    "commands": [
      {
        "command": "KitchenSink.openWithBreadboardEditor",
        "title": "Open with Breadboard editor",
        "category": "Kitchen Sink"
      },
      {
        "command": "KitchenSink.openWithEditor",
        "title": "Open with KitchenSink editor",
        "category": "Kitchen Sink"
      },
      {
        "command": "KitchenSink.truncateListing",
        "title": "truncateListing",
        "category": "Kitchen Sink"
      },
      {
        "command": "KitchenSink.resetListing",
        "title": "resetListing",
        "category": "Kitchen Sink"
      },
      {
        "command": "KitchenSink.kickerOpen",
        "title": "Open kicker",
        "category": "Kitchen Sink"
      },
      {
        "command": "KitchenSink.wizardOpen",
        "title": "Open wizard",
        "category": "Kitchen Sink"
      }
    ]
  },
  "activationEvents": [
    "onCustomEditor:KitchenSink.EditorProvider",
    "onCommand:KitchenSink.openWithEditor",
    "onCustomEditor:KitchenSink.BreadboardEditorProvider",
    "onCommand:KitchenSink.openWithBreadboardEditor",
    "onCommand:KitchenSink.recenter",
    "onCommand:KitchenSink.truncateListing",
    "onCommand:KitchenSink.resetListing",
    "onCommand:KitchenSink.kickerOpen",
    "onCommand:KitchenSink.wizardOpen"
  ],
  "scripts": {
    "extension-build": "node esbuild-extension.js",
    "extension-watch": "node esbuild-extension.js --watch --sourcemap",
    "extension-web-build": "node esbuild-extension.js --web",
    "extension-web-watch": "node esbuild-extension.js --web --watch --sourcemap",
    "extension-build-prod": "node esbuild-extension.js --minify && node esbuild-extension.js --noclean --web --minify",
    "standalone-build": "mkdir -p dist/standalone && rm -rf dist/standalone/* && cp -r src/standalone/* dist/standalone",
    "webview-build": "node esbuild-webview.js",
    "webview-watch": "node esbuild-webview.js --watch --sourcemap"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@vscode/codicons": "^0.0.27",
    "esbuild": "^0.12.25",
    "immutable": "^3.8.1",
    "jsonschema": "^1.4.0",
    "svelte": "^3.42.4",
    "svelte-forms": "^2.2.1",
    "throttle-debounce": "^3.0.1"
  }
}
